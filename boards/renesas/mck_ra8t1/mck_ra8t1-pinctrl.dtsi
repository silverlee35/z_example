&pinctrl {
	sci3_default: sci3_default {
		group1 {
			/* tx */
			psels = <RA_PSEL(RA_PSEL_SCI_3, 3, 10)>;
			drive-strength = "medium";
		};
		group2 {
			/* rx */
			psels = <RA_PSEL(RA_PSEL_SCI_3, 3, 9)>;
		};
	};

	spi0_default: spi0_default {
		group1 {
			/* MISO MOSI RSPCK SSL*/
			psels = <RA_PSEL(RA_PSEL_SPI, 3, 13)>,
			<RA_PSEL(RA_PSEL_SPI, 2, 2)>,
			<RA_PSEL(RA_PSEL_SPI, 2, 3)>,
			<RA_PSEL(RA_PSEL_SPI, 2, 4)>;
		};
	};

	pwm2_default: pwm2_default {
		group1 {
			/* GTIOC2A */
			psels = <RA_PSEL(RA_PSEL_GPT1, 1, 13)>;
		};
		group2 {
			/* GTIOC2B */
			psels = <RA_PSEL(RA_PSEL_GPT1, 1, 14)>;
		};
	};

	sdmmc0_default: sdmmc0_default {
		group1 {
			/* SDCD SDCMD SDDATA0 SDDATA1 SDDATA2 SDDATA3 SDWP */
			psels = <RA_PSEL(RA_PSEL_SDHI, 3, 6)>,
			<RA_PSEL(RA_PSEL_SDHI, 3, 7)>,
			<RA_PSEL(RA_PSEL_SDHI, 3, 4)>,
			<RA_PSEL(RA_PSEL_SDHI, 3, 3)>,
			<RA_PSEL(RA_PSEL_SDHI, 3, 2)>,
			<RA_PSEL(RA_PSEL_SDHI, 3, 1)>,
			<RA_PSEL(RA_PSEL_SDHI, 3, 5)>;
			drive-strength = "high";
		};
		group2 {
			/* SDCLK */
			psels = <RA_PSEL(RA_PSEL_SDHI, 3, 8)>;   /* P308 */
			drive-strength = "highspeed-high";
		};
	};
};
